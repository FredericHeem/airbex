server {
    listen <%= node[:snow][:frontend][:port] %>;
    server_name <%= node[:snow][:frontend][:domain] %>;
    root /home/ubuntu/snow-frontend/current/frontend/dist/;
    access_log /var/log/snow-frontend-access.log;
    error_log /var/log/snow-frontend-error.log;

    gzip on;
    gzip_http_version 1.1;
    gzip_vary on;
    gzip_comp_level 6;
    gzip_proxied any;
    gzip_types text/plain text/css application/json application/x-javascript text/xml application/xml application/xml+rss text/javascript application/javascript text/x-js;
    gzip_buffers 16 8k;
    gzip_disable "MSIE [1-6]\.(?!.*SV1)";
}
